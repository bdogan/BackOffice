{
    "name": "bdogan/back-office",
    "description": "BackOffice plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^4.1",
        "wyrihaximus/twig-view": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "BackOffice\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BackOffice\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
